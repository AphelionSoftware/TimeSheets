<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>Package</WebPublishMethod>
    <UseLightSwitchWebDeploymentProvider>True</UseLightSwitchWebDeploymentProvider>
    <AuthenticationType>None</AuthenticationType>
    <RequiresIntrinsicDataSource>false</RequiresIntrinsicDataSource>
    <DeployDatabase>False</DeployDatabase>
    <LightSwitchPublishVersion>v1.8</LightSwitchPublishVersion>
    <PackageLocation>Publish//TimesheetsAdmin.zip</PackageLocation>
    <CleanWPPAllFilesInSingleFolder>False</CleanWPPAllFilesInSingleFolder>
    <ExcludeGeneratedDebugSymbol>true</ExcludeGeneratedDebugSymbol>
    <DisableAllVSGeneratedMSDeployParameter>True</DisableAllVSGeneratedMSDeployParameter>
    <AutoParameterizationWebConfigConnectionStrings>True</AutoParameterizationWebConfigConnectionStrings>
    <LightSwitchExcludeSetParameter>{LightSwitchApplication_Path}</LightSwitchExcludeSetParameter>
    <WPPAllFilesInSingleFolder>../Bin/Debug/app.publish/</WPPAllFilesInSingleFolder>
    <PackageSourceManifest>../Bin/Debug/TimesheetsAdmin.xml</PackageSourceManifest>
    <ProjectParametersXMLFile>..//TimesheetsAdmin.Server/generated.parameters.xml</ProjectParametersXMLFile>
    <LightSwitchSqlScriptFile>../Bin/Debug/TimesheetsAdmin.sql</LightSwitchSqlScriptFile>
  </PropertyGroup>
  <Target Name="BeforeBuild">
    <MSBuild Projects="../TimesheetsAdmin.ls3proj" Targets="BeforeLightSwitchPublishApplication" />
  </Target>
</Project>