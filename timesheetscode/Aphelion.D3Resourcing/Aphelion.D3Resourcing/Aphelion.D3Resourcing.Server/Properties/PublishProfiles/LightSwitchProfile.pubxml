<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <UseLightSwitchWebDeploymentProvider>False</UseLightSwitchWebDeploymentProvider>
    <AuthenticationType>None</AuthenticationType>
    <MSDeployServiceURL>waws-prod-am2-003.publish.azurewebsites.windows.net:443</MSDeployServiceURL>
    <DeployIisAppPath>Aphelion-TestOData</DeployIisAppPath>
    <AllowUntrustedCertificate>False</AllowUntrustedCertificate>
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <DeployAsIisApp>True</DeployAsIisApp>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <UserName>$Aphelion-TestOData</UserName>
    <SavePWD>True</SavePWD>
    <LightSwitchPublishVersion>v2.2</LightSwitchPublishVersion>
    <CleanWPPAllFilesInSingleFolder>False</CleanWPPAllFilesInSingleFolder>
    <ExcludeGeneratedDebugSymbol>true</ExcludeGeneratedDebugSymbol>
    <AutoParameterizationWebConfigConnectionStrings>False</AutoParameterizationWebConfigConnectionStrings>
    <WPPAllFilesInSingleFolder>../bin/Debug/app.publish/</WPPAllFilesInSingleFolder>
  </PropertyGroup>
  <Target Name="BeforeBuild">
    <MSBuild Projects="../Aphelion.D3Resourcing.lsxproj" Targets="BeforeLightSwitchPublishApplication" />
  </Target>
</Project>