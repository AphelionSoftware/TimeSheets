      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT CAST ( [BillingStatusName] as nvarchar(255)) BillingStatusName&#xA;       ,CAST ( [ClientName] as nvarchar(255)) ClientName&#xA;&#x9;   ,CAST ( [ProjectName] as nvarchar(255)) ProjectName&#xA;      ,[TimesheetDetailID]&#xA;      ,CAST ( [TimesheetDetailSourceKey] as nvarchar(255)) TimesheetDetailSourceKey&#xA;      ,[LoadDate]&#xA;      ,[Active]&#xA;      ,CAST ( [TimesheetDetailFileName] as nvarchar(255)) TimesheetDetailFileName&#xA;      ,[TimesheetDetailDateID]&#xA;      ,[LoadDateID]&#xA;      ,[Hours]&#xA;      ,[TimesheetID]&#xA;      ,CAST ( [Comments] as nvarchar(255)) Comments&#xA;      ,CAST ( [ClientComments] as nvarchar(255)) ClientComments&#xA;      ,CAST ( [AMComments] as nvarchar(255)) AMComments&#xA;      ,[Exclude]&#xA;      ,CAST ( [TypeOfWorkName] as nvarchar(255)) TypeOfWorkName&#xA;      ,CAST ( [AccountManager] as nvarchar(255)) AccountManager&#xA;      ,CAST ( [AMEmail] as nvarchar(255)) AMEmail&#xA;      ,CAST ( [BillingPeriodText] as nvarchar(255)) BillingPeriodText&#xA;  FROM [dbo].[UnallocatedTimesheets]&#xA;WHERE AccountManager = '&quot; +  @[User::AM_Name] + &quot;'&#xA;&#xA;ORDER BY ClientName, ProjectName, TimesheetDetailSourceKey, TimesheetDetailDateID, Comments&#xA;&quot;"
